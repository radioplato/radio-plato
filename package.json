{
  "name": "radio-plato",
  "version": "1.2.1",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.35",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-helmet": "^6.0.0",
    "@types/react-router-dom": "^5.1.4",
    "@types/react-slider": "^1.0.0",
    "env-cmd": "^10.1.0",
    "moment": "^2.29.2",
    "react": "^16.13.1",
    "react-device-detect": "^1.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.0.6",
    "react-markdown": "^4.3.1",
    "react-moment": "^0.9.7",
    "react-player": "^1.15.3",
    "react-responsive-carousel": "^3.2.9",
    "react-router-dom": "^5.1.2",
    "react-router-ga": "^1.2.3",
    "react-scripts": "^3.4.3",
    "react-slider": "^1.0.8",
    "rxjs": "^6.5.5",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "start": "cross-env PUBLIC_URL='.' env-cmd -f .env.local react-scripts start",
    "build": "cross-env env-cmd -f .env.production react-scripts build",
    "build:staging": "cross-env PUBLIC_URL='/radio-plato' env-cmd -f .env.staging react-scripts build",
    "predeploy": "npm run build:staging",
    "deploy:staging": "gh-pages -d build",
    "postbuild": "react-snap"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-dev-shm-usage"
    ],
    "puppeteer": {
      "waitUntil": "networkidle2"
    },
    "concurrency": 1,
    "puppeteerExecutablePath": "/usr/bin/chromium-browser",
    "inlineCss": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@iconify/icons-bx": "^1.0.2",
    "@iconify/icons-cib": "^1.0.1",
    "@iconify/icons-el": "^1.0.4",
    "@iconify/icons-gridicons": "^1.0.5",
    "@iconify/icons-ps": "^1.0.3",
    "@iconify/icons-simple-icons": "^1.0.31",
    "@iconify/react": "^1.1.3",
    "cross-env": "^7.0.2",
    "gh-pages": "^2.2.0",
    "react-snap": "1.17.0"
  }
}
